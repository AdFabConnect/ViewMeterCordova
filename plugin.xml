<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="fr.adfab.Volumedetectionphonegap" version="1.1.0">
	
    <engines>
        <engine name="cordova" version="3.*" />
    </engines>
    
    <name>VolumeDetectionPhonegap</name>
    <description>VolumeDetectionPhonegap</description>
    <license>MIT</license>
    <keywords>Sound,meter</keywords>

    <platform name="ios">
        <config-file target="config.xml" parent="/*">
		    <feature name="VolumeDetectionPhonegap">
		        <param name="ios-package" value="VolumeDetectionPhonegap" />
		        <param name="stop" value="VolumeDetectionPhonegap" />
		        <param name="start" value="VolumeDetectionPhonegap" />
		    </feature>
        </config-file>

        <header-file src="src/ios/VolumeDetectionPhonegap.h" />
        <source-file src="src/ios/VolumeDetectionPhonegap.m" />
        
        <js-module src="www/ios.js" name="VolumeDetectionPhonegap">
            <clobbers target="window.VolumeDetectionPhonegap" />
        </js-module>
    </platform>
</plugin>